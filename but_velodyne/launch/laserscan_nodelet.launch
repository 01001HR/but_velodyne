<?xml version="1.0"?>
<launch>
  <arg name="calibration" default="" />
  <node pkg="nodelet" type="nodelet" name="laserscan_nodelet"
        args="load but_velodyne/LaserScanNodelet velodyne_nodelet_manager" >
    <remap from="points_in" to="/velodyne_points" />
    <remap from="scan_out" to="/sim_laser_scan" />
    <!--param name="min_z" value="-100.0" /-->
    <param name="min_z" value="0.0" />
    <param name="max_z" value="100.0" />
    <param name="angular_res" value="0.1" />
    <!--param name="frame_id" value="/velodyne" /-->
  </node>
</launch>
